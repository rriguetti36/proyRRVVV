<main class="container-fluid py-4">

    <div class="card shadow-sm">
        <div class="card-header bg-primary text-white">

            <div class="row mb-3">
                <input type="hidden" id="idcot" value="<%= datacot.id_cot %>">
                <div class="col-md-2">
                    <label>Nro Poliza</label>
                    <input type="text" class="form-control" id="numpol" value="<%= datacot.num_pol %>" disabled>
                </div>
                <div class="col-md-2">
                    <label>Fecha Emisión</label>
                    <input type="date" class="form-control" id="fecemi" value="<%= datacot.fec_emision %>" disabled>
                </div>
                <div class="col-md-2">
                    <label>CUSPP</label>
                    <input type="text" class="form-control" id="cussp" value="<%= datacot.num_cuspp %>" disabled>
                </div>
                <div class="col-md-2">
                    <label>Nro Cotizacion</label>
                    <input type="text" class="form-control" id="numcot" value="<%= datacot.num_cot %>" disabled>
                </div>
                <div class="col-md-2">
                    <label>Nro Operación</label>
                    <input type="text" class="form-control" id="numope" value="<%= datacot.num_operacion %>" disabled>
                </div>
                <div class="col-md-2">
                    <label>Ingreso SPP</label>
                    <input type="date" class="form-control" id="fecspp" value="<%= datacot.fec_ingresospp %>">
                </div>
            </div>
        </div>

        <div class="card-body">
            <!-- Tabs -->
            <ul class="nav nav-tabs" id="polizasTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <button class="nav-link active" id="afiliado-tab" data-bs-toggle="tab" data-bs-target="#afiliado"
                        type="button" role="tab">Datos de Afiliado</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="domicilio-tab" data-bs-toggle="tab" data-bs-target="#domicilio"
                        type="button" role="tab">Domicilio</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="poliza-tab" data-bs-toggle="tab" data-bs-target="#poliza" type="button"
                        role="tab">Datos de la Poliza</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="pagos-tab" data-bs-toggle="tab" data-bs-target="#pagos" type="button"
                        role="tab">Formas de Pago Pension</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="beneficiarios-tab" data-bs-toggle="tab" data-bs-target="#beneficiarios"
                        type="button" role="tab">Beneficiarios</button>
                </li>
            </ul>

            <div class="tab-content p-3 border border-top-0 rounded-bottom bg-light" id="polizasTabContent">
                <!-- AFILIADO -->
                <div class="tab-pane fade show active" id="afiliado" role="tabpanel">
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Tipo de Documento</label>
                            <select class="form-control" id="tipodocafi" disabled>
                                <% tipodoc.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Documento</label>
                            <input type="text" class="form-control" id="Documentoafi" disabled>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Género</label>
                            <select class="form-control" id="tiposexafi" disabled>
                                <% tiposex.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Nacionalidad</label>
                            <select class="form-control" id="Nacionalidadfi" required>
                                <option value="1">PERUANA</option>
                                <option value="2">EXTRANJERA</option>
                            </select>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Apellido Paterno</label>
                            <input type="text" class="form-control" id="apepatafi" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Apellido Materno</label>
                            <input type="text" class="form-control" id="apematafi" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Primer Nombre</label>
                            <input type="text" class="form-control" id="nombreafi" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Segundo Nombre</label>
                            <input type="text" class="form-control" id="nombresegafi">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Fecha de Nacimiento</label>
                            <input type="date" class="form-control" id="fecnacafi" disabled>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Fecha de Fallecimiento</label>
                            <input type="date" class="form-control" id="fecfalafi" required>
                        </div>
                    </div>
                </div>

                <!-- DOMICILIO -->
                <div class="tab-pane fade" id="domicilio" role="tabpanel">

                    <div class="row mb-3">
                        <div class="form-group col-md-6">
                            <label>Dirección Expediente</label>
                            <input type="text" id="direxpafi" class="form-control" placeholder="Escribe una dirección...">
                        </div>
                        <div class="form-group col-md-6">
                            <label>Ubigeo Expediente</label>
                            <div class="input-group mb-2">
                                <input type="hidden" id="iddirexpafi">
                                <input type="text" id="txtUbigeoExp" class="form-control"
                                    placeholder="Ubigeo correspondencia..." readonly>
                                <button type="button" class="btn btn-outline-info btnUbigeo" 
                                    
                                    data-bs-target="#modalUbigeo" 
                                    data-targetinput="txtUbigeoExp"
                                    data-targetid="iddirexpafi">
                                    <i class="fas fa-map-marker-alt"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-6">
                            <label>Dirección Correspondencia</label>
                            <input type="text" id="dircorafi" class="form-control" placeholder="Escribe una dirección...">
                        </div>
                        <div class="form-group col-md-6">
                            <label>Ubigeo Correspondencia</label>
                            <div class="input-group mb-2">
                                <input type="hidden" id="iddircorafi">
                                <input type="text" id="txtUbigeoCor" class="form-control"
                                    placeholder="Ubigeo correspondencia..." readonly>
                                <button type="button" class="btn btn-outline-info btnUbigeo" 
                                    
                                    data-bs-target="#modalUbigeo" 
                                    data-targetinput="txtUbigeoCor"
                                    data-targetid="iddircorafi">
                                    <i class="fas fa-map-marker-alt"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-4">
                            <label>Correo Electrónico</label>
                            <input type="email" id="correoafi1" class="form-control" placeholder="Escribe un email..." required>
                        </div>
                        <div class="form-group col-md-4">
                            <label>Correo Electrónico 2</label>
                            <input type="email" id="correoafi2" class="form-control" placeholder="Escribe un email..." required>
                        </div>
                        <div class="form-group col-md-4">
                            <label>Correo Electrónico 3</label>
                            <input type="email" id="correoafi3" class="form-control" placeholder="Escribe un email..." required>
                        </div>

                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-4">
                            <label>Teléfono</label>
                            <input type="text" id="telefonoafi1" class="form-control" placeholder="Escribe un numero...">
                        </div>
                        <div class="form-group col-md-4">
                            <label>Teléfono 2</label>
                            <input type="text" id="telefonoafi2" class="form-control" placeholder="Escribe un numero...">
                        </div>
                        <div class="form-group col-md-4">
                            <label>Teléfono 3</label>
                            <input type="text" id="telefonoafi3" class="form-control" placeholder="Escribe un numero...">
                        </div>
                    </div>
                </div>

                <!-- POLIZA -->
                <div class="tab-pane fade" id="poliza" role="tabpanel">
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Tipo Pensión</label>
                            <select class="form-control" id="tipopenafi" disabled>
                                <% tipoPen.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Fec Invalidez</label>
                            <input type="date" id="fecinvafi" class="form-control">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Causa Invalidez</label>
                            <select class="form-control" id="cauinvafi" disabled>
                                <option>Sin Información</option>
                                <% tipoEnf.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Tipo Vejez</label>
                            <select class="form-control" id="tipovejafi" disabled>
                                <option>Sin Información</option>
                                <% tipoVej.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>AFP</label>
                            <select class="form-control" id="tipoAfpafi" disabled>
                                <% tipoAfp.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Est. Civil</label>
                            <select class="form-control" id="tipoCivafi">
                                <% tipoCiv.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Ins. Salud</label>
                            <select class="form-control" id="tipoEssafi">
                                <% tipoEss.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Representante</label>
                            <input type="text" id="Representanteafi" class="form-control">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Fec. aceptación</label>
                            <input type="date" id="fecaceptaafi" class="form-control">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Marcar :</label>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="protecdatafi" name="protecdatafi"
                                    value="1">
                                <label class="form-check-label" for="protecdatafi">Protección de Datos</label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="boletapagafi" name="boletapagafi"
                                    value="1">
                                <label class="form-check-label" for="boletapagafi">Boleta de Pago</label>
                            </div>
                        </div>
                    </div>

                </div>

                <!-- PAGOS -->
                <div class="tab-pane fade" id="pagos" role="tabpanel">
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Vía Pago</label>
                            <select class="form-control" id="tipoPagafi">
                                <% tipoPag.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Sucursal</label>
                            <select class="form-control" id="tipoAfpafi">
                                <% tipoAfp.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Banco</label>
                            <select class="form-control" id="tipoBcoafi">
                                <% tipoBco.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Tipo Cuenta</label>
                            <select class="form-control" id="tipoctaafi">
                                <% tipocta.forEach(function(doc) { %>
                                    <option value="<%= doc.v_cod %>">
                                        <%= doc.v_nombre %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-md-3">
                            <label>Número Cuenta</label>
                            <input type="text" id="numeroctaafi" class="form-control" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Número CCI</label>
                            <input type="text" id="numerocciafi" class="form-control" required>
                        </div>
                    </div>
                </div>

                <!-- BENEFICIARIOS -->
                <div class="tab-pane fade" id="beneficiarios" role="tabpanel">
                    <div class="table-responsive" style="max-height: 200px; overflow-y: auto;">
                        <table class="table table-bordered table-hover align-middle text-center"
                            id="tablaBeneficiarios">
                            <thead class="table-light sticky-header">
                                <tr>
                                    <th>N° Orden</th>
                                    <th>Parentesco</th>
                                    <th>Nombres</th>
                                    <th>Tipo Doc.</th>
                                    <th>Nro Doc.</th>
                                    <th>Acciones</th>
                                </tr>
                            </thead>
                            <tbody>

                            </tbody>
                        </table>
                    </div>
                    <br>
                    <ul class="nav nav-tabs" id="benefTab" role="tablist">
                        <li class="nav-item" role="presentation">
                            <button class="nav-link active" id="benef-tab" data-bs-toggle="tab" data-bs-target="#benef"
                                type="button" role="tab">Datos de Beneficiario</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link" id="domicilioben-tab" data-bs-toggle="tab"
                                data-bs-target="#domicilioben" type="button" role="tab">Datos Domicilio, correo y
                                telefono</button>
                        </li>
                    </ul>
                    <div class="tab-content p-3 border border-top-0 rounded-bottom bg-light" id="benefTabContent">
                        <div class="tab-pane fade show active" id="benef" role="tabpanel">
                            <div class="row mb-3">
                                <div class="form-group col-md-3">
                                    <label>Tipo Ident.</label>
                                    <select class="form-control" id="tipodocben">
                                        <% tipodoc.forEach(function(doc) { %>
                                            <option value="<%= doc.v_cod %>">
                                                <%= doc.v_nombre %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>N° Ident.</label>
                                    <input type="text" class="form-control" id="numeroidenben">
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Primer Nombre</label>
                                    <input type="text" class="form-control" id="nombreben">
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Segundo Nombre</label>
                                    <input type="text" class="form-control" id="nombresegben">
                                </div>
                            </div>
                            <div class="row mb-3">
                                <div class="form-group col-md-3">
                                    <label>Ap. Paterno</label>
                                    <input type="text" class="form-control" id="apepatben">
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Ap. Materno</label>
                                    <input type="text" class="form-control" id="apematben">
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Grupo Familiar</label>
                                    <select class="form-control" id="grupofamben" disabled>
                                        <% tipoGru.forEach(function(doc) { %>
                                            <option value="<%= doc.v_cod %>">
                                                <%= doc.v_nombre %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Género</label>
                                    <select class="form-control" id="sexoben" disabled>
                                        <% tiposex.forEach(function(doc) { %>
                                            <option value="<%= doc.v_cod %>">
                                                <%= doc.v_nombre %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>
                            </div>
                            <div class="row mb-3">
                                <div class="form-group col-md-3">
                                    <label>Fec. Nac.</label>
                                    <input type="date" class="form-control" id="nacimientoben" disabled>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Fec. Fall.</label>
                                    <input type="date" class="form-control" id="fallecimientoben">
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Sit. Invalidez</label>
                                    <select class="form-control" id="invalidezben" disabled>
                                        <% tipoInv.forEach(function(doc) { %>
                                            <option value="<%= doc.v_cod %>">
                                                <%= doc.v_nombre %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Causa Invalidez</label>
                                    <select class="form-control" id="causainvalidezben">
                                        <option>Sin Información</option>
                                        <% tipoEnf.forEach(function(doc) { %>
                                            <option value="<%= doc.v_cod %>">
                                                <%= doc.v_nombre %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>
                            </div>
                            <div class="row mb-3">
                                <div class="form-group col-md-3">
                                    <label>Fec. Invalidez</label>
                                    <input type="date" class="form-control" id="fechainvalidezben">
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Parentesco</label>
                                    <select class="form-control" id="parentescoben" disabled>
                                        <% tipoPar.forEach(function(doc) { %>
                                            <option value="<%= doc.v_cod %>">
                                                <%= doc.v_nombre %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Derecho a Pensión</label>
                                    <select class="form-control" id="derpenben" disabled>
                                        <option value="1">Si</option>
                                        <option value="2">No</option>
                                    </select>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Beneficio Estudiante :</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="estudiantesi"
                                            name="estudiantesi" value="1">
                                        <label class="form-check-label" for="estudiantesi">Estudiante</label>
                                    </div>
                                </div>
                            </div>
                            <div class="row mb-3">
                                <div class="form-group col-md-3">
                                    <label>% Pensión</label>
                                    <input type="text" class="form-control" id="prcpenben" disabled>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Pensión</label>
                                    <input type="text" class="form-control" id="montopenben" disabled>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Marcar :</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="protecdatosben"
                                            name="protecdatosben" value="1">
                                        <label class="form-check-label" for="protecdatosben">Protección de Datos</label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="boletapagben"
                                            name="boletapagben" value="1">
                                        <label class="form-check-label" for="boletapagben">Boleta de Pago</label>
                                    </div>
                                </div>
                                <div class="form-group col-md-3">
                                    <label>Marcar en caso de Exclusiones :</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="excluyefall"
                                            name="excluyefall" value="1">
                                        <label class="form-check-label" for="excluyefall">Por Fallecimiento</label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="excluyemayor"
                                            name="excluyemayor" value="1">
                                        <label class="form-check-label" for="excluyemayor">Por Mayoria de edad</label>
                                    </div>
                                </div>
                            </div>
                            <div class="text-center mt-4">
                                <button type="reset" class="btn btn-secondary"><i class="fas fa-times"></i>
                                    Cancelar/Limpiar</button>
                                <button type="submit" id="btnActualizarBen" class="btn btn-primary ml-2"><i
                                        class="fas fa-save"></i>
                                    Actualizar Datos</button>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="domicilioben" role="tabpanel">
                            <div class="row mb-3">
                                <div class="form-group col-md-6">
                                    <label>Dirección Expediente</label>
                                    <input type="text" id="txtDireccionoExp" class="form-control"
                                        placeholder="Escribe una dirección...">
                                </div>
                                <div class="form-group col-md-6">
                                    <label>Ubigeo Expediente</label>
                                    <div class="input-group mb-2">
                                        <input type="hidden" id="iddirexp">
                                        <input type="text" id="txtUbigeoExpBen" class="form-control"
                                            placeholder="Ubigeo expediente..." readonly>
                                        <button type="button" class="btn btn-outline-info btnUbigeo"
                                            
                                            data-bs-target="#modalUbigeo"
                                            data-targetinput="txtUbigeoExpBen" 
                                            data-targetid="iddirexp">
                                            <i class="fas fa-map-marker-alt"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="row mb-3">
                                <div class="form-group col-md-6">
                                    <label>Dirección Correspondencia</label>
                                    <input type="text" id="txtDireccionCor" class="form-control"
                                        placeholder="Escribe una dirección...">
